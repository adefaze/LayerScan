{
    "name": "layer-intelligence",
    "version": "1.0.0",
    "description": "A smart design linter for Framer that surfaces layout, spacing, accessibility, and performance issues with one-click fixes",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "jest",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "format": "prettier --write \"src/**/*.{ts,tsx,css}\""
    },
    "dependencies": {
        "framer-plugin": "^0.5.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^14.2.1",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.2",
        "vite": "^5.1.6",
        "vite-plugin-framer": "^1.0.7"
    },
    "keywords": [
        "framer",
        "plugin",
        "design-linter",
        "accessibility",
        "layout"
    ],
    "author": "",
    "license": "MIT"
}
